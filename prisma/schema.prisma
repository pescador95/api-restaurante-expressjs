generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model cliente {
  id             BigInt    @id
  nome           String    @db.VarChar(255)
  cpf            String    @unique @db.VarChar(15)
  telefone       String    @db.VarChar(20)
  datanascimento DateTime? @db.Timestamp(6)
  endereco       String    @db.VarChar(255)
}
